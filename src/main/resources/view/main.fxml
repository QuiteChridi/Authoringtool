<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1">
    <TabPane fx:id="tabPane" tabClosingPolicy="UNAVAILABLE" AnchorPane.leftAnchor="0" AnchorPane.rightAnchor="0">
        <Tab text="User">
            <fx:include source="/view/user.fxml"/>
        </Tab>
        <Tab text="Joker">
            <fx:include source="/view/joker.fxml"/>
        </Tab>
        <Tab text="Questions">
            <fx:include source="/view/question.fxml"/>
        </Tab>
        <Tab text="Stats">
            <fx:include source="/view/stats.fxml"/>
        </Tab>
    </TabPane>
</AnchorPane>

