<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="controller.UserController">

    <TableView fx:id="userTable" prefHeight="400.0" prefWidth="600.0">
        <columns>
            <TableColumn fx:id="userIdColumn" text="User ID" prefWidth="75.0"/>
            <TableColumn fx:id="usernameColumn" text="Username" prefWidth="150.0"/>
            <TableColumn fx:id="emailColumn" text="Email" prefWidth="150.0"/>
            <TableColumn fx:id="passwordColumn" text="Password" prefWidth="150.0"/>
        </columns>
    </TableView>

</AnchorPane>
