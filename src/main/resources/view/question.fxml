<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="715.0" prefWidth="1037.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.QuestionController">
    <VBox alignment="CENTER" prefHeight="715.0" prefWidth="1037.0" VBox.vgrow="ALWAYS">
        <TableView fx:id="questionTable" prefHeight="671.0" prefWidth="1038.0">
            <columns>
                <TableColumn fx:id="quizIdColumn" minWidth="75.0" prefWidth="75.0" text="Quiz ID" />
                <TableColumn fx:id="questionColumn" minWidth="300.0" prefWidth="300.0" text="Question" />
                <TableColumn fx:id="correctAnswerColumn" minWidth="150.0" prefWidth="150.0" text="Correct Answer" />
                <TableColumn fx:id="wrongAnswer1Column" minWidth="150.0" prefWidth="150.0" text="Wrong Answer 1" />
                <TableColumn fx:id="wrongAnswer2Column" minWidth="150.0" prefWidth="150.0" text="Wrong Answer 2" />
                <TableColumn fx:id="wrongAnswer3Column" minWidth="150.0" prefWidth="150.0" text="Wrong Answer 3" />
            </columns>
        </TableView>
        <HBox alignment="CENTER" prefHeight="0.0" prefWidth="241.0" spacing="10">
            <Button fx:id="edit" onAction="#onEditButtonClick" text="Edit" />
            <Button fx:id="user" onAction="#onUserButtonClick" text="User" />
            <Button text="New" />
        </HBox>
    </VBox>

</AnchorPane>
